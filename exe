#include <iostream>

using namespace std;

int main() {
	int a;
	a = 10;

	if (a <= 12) {
		cout << "Good Morning" << endl;
	}
	if (a <= 18) {
		cout << "Good Afternoon" << endl;
	}
	if (a <= 21) {
		cout << "Good Evening" << endl;
	}
	if (a <= 24) {
		cout << "Good Night" << endl;
	}
	else {
		cout << "Time not Valid" << endl;
	}cin.get();
	return 0;

}
